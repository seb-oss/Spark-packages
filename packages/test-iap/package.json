{
  "name": "@sebspark/test-iap",
  "version": "1.0.4",
  "license": "Apache-2.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn build:index && yarn build:start",
    "build:index": "tsup src/index.ts --config ./tsup.config.ts",
    "build:start": "tsup src/start.ts --config ./tsup.config.ts",
    "dev": "tsc --watch --noEmit",
    "lint": "biome check .",
    "test": "vitest run --passWithNoTests --coverage",
    "pretest:e2e": "yarn build",
    "test:e2e": "vitest --config vitest.config.e2e.ts --run",
    "typecheck": "vitest --typecheck.only --passWithNoTests"
  },
  "dependencies": {
    "jose": "6.1.0",
    "testcontainers": "11.9.0"
  },
  "devDependencies": {
    "@sebspark/tsconfig": "*",
    "@types/ws": "8.18.1",
    "ws": "8.18.3"
  }
}
