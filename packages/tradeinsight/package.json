{
  "name": "@sebspark/tradeinsight",
  "version": "5.0.1",
  "license": "Apache-2.0",
  "type": "module",
  "exports": {
    "./avro": {
      "types": "./dist/avro-schemas.d.mts",
      "import": "./dist/avro-schemas.mjs",
      "react-native": "./dist/avro-schemas.mjs"
    },
    "./broker": {
      "types": "./dist/market-data-broker.d.mts",
      "import": "./dist/market-data-broker.mjs",
      "react-native": "./dist/market-data-broker.mjs"
    },
    "./openapi": {
      "types": "./dist/openapi.d.mts",
      "import": "./dist/openapi.mjs",
      "react-native": "./dist/openapi.mjs"
    },
    "./subscriber": {
      "types": "./dist/market-data-subscriber.d.mts",
      "import": "./dist/market-data-subscriber.mjs",
      "react-native": "./dist/market-data-subscriber.mjs"
    },
    "./utils": {
      "types": "./dist/utils.d.mts",
      "import": "./dist/utils.mjs",
      "react-native": "./dist/utils.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "spark-build src/market-data-broker.ts src/market-data-subscriber.ts src/avro-schemas.ts",
    "depcheck": "depcheck",
    "dev": "tsc --watch --noEmit",
    "typegen": "openapi-typegen -i ./schemas -o ./src/generated/",
    "lint": "biome check .",
    "test": "vitest run --passWithNoTests --coverage",
    "test:e2e": "vitest --config vitest.config.e2e.ts --run",
    "typecheck": "vitest --typecheck.only --passWithNoTests"
  },
  "dependencies": {
    "@sebspark/avsc-isometric": "*",
    "@sebspark/openapi-core": "*",
    "@sebspark/otel": "*",
    "@sebspark/socket.io-avro": "*"
  },
  "peerDependencies": {
    "@google-cloud/pubsub": "5.2.0",
    "socket.io": "4.8.1",
    "socket.io-client": "4.8.1"
  },
  "devDependencies": {
    "@google-cloud/pubsub": "5.2.0",
    "@sebspark/openapi-typegen": "*",
    "@sebspark/tsconfig": "*",
    "@testcontainers/gcloud": "11.10.0",
    "find-free-ports": "3.1.1",
    "google-gax": "5.0.6",
    "socket.io": "4.8.3",
    "socket.io-client": "4.8.3"
  }
}
