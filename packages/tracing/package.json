    "@opentelemetry/instrumentation-http": "0.56.0",
    "@opentelemetry/instrumentation-pg": "0.41.0",
    "@opentelemetry/instrumentation-socket.io": "0.45.0",
    "@opentelemetry/instrumentation-undici": "0.7.0",
