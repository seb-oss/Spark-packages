{
  "name": "@sebspark/cli-tester",
  "version": "1.0.1",
  "license": "Apache-2.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup-node src/index.ts --format esm --target node22 --dts",
    "dev": "tsc --watch --noEmit",
    "lint": "biome check .",
    "test:e2e": "vitest run --passWithNoTests --coverage",
    "typecheck": "vitest --typecheck.only --passWithNoTests"
  },
  "devDependencies": {
    "@inquirer/prompts": "7.9.0",
    "@sebspark/tsconfig": "*"
  }
}
