{
  "name": "@sebspark/openapi-client",
  "version": "4.1.3",
  "license": "Apache-2.0",
  "type": "module",
  "main": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "generate": "yarn openapi-typegen -i ./src/__tests__/ -o ./src/__tests__/",
    "build": "spark-build src/index.ts",
    "depcheck": "depcheck",
    "dev": "tsc --watch --noEmit",
    "lint": "biome check .",
    "test": "vitest run --passWithNoTests --coverage",
    "pretest": "yarn generate",
    "typecheck": "vitest --typecheck.only --passWithNoTests"
  },
  "devDependencies": {
    "@sebspark/openapi-typegen": "5.0.12",
    "express": "5.2.1",
    "nock": "14.0.11"
  },
  "dependencies": {
    "@sebspark/openapi-core": "4.0.13",
    "@sebspark/otel": "2.0.11",
    "@sebspark/retry": "1.0.6",
    "axios": "1.13.5",
    "axios-auth-refresh": "3.3.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seb-oss/Spark-packages.git"
  }
}
