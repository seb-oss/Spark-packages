{
  "name": "@sebspark/tsconfig",
  "version": "2.1.6",
  "license": "Apache-2.0",
  "type": "module",
  "files": [
    "base.json",
    "tsconfig.app.json",
    "tsconfig.lib.json",
    "tsup.js",
    "tsup.d.ts",
    "tsdown.js",
    "tsdown.d.ts"
  ],
  "exports": {
    "./tsup": {
      "types": "./tsup.d.ts",
      "import": "./tsup.js"
    },
    "./tsdown": {
      "types": "./tsdown.d.ts",
      "import": "./tsdown.js"
    },
    "./tsconfig.app.json": "./tsconfig.app.json",
    "./tsconfig.lib.json": "./tsconfig.lib.json",
    "./base.json": "./base.json",
    "./run.js": "./run.js"
  },
  "bin": {
    "spark-build": "./run.js"
  },
  "dependencies": {
    "tsdown": "0.16.6",
    "tsup": "8.5.1"
  },
  "installConfig": {
    "hoistingLimits": "workspaces"
  }
}
