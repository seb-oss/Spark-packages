{
  "name": "@sebspark/spanner-migrate",
  "version": "2.0.5",
  "license": "Apache-2.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "bin": "./dist/cli.js",
  "scripts": {
    "build": "tsup src/index.ts src/cli.ts --config ./tsup.config.ts",
    "dev": "tsc --watch --noEmit",
    "lint": "biome check .",
    "test": "vitest run --passWithNoTests --coverage",
    "test:e2e": "vitest --config vitest.config.e2e.ts --run",
    "typecheck": "tsc --noEmit "
  },
  "devDependencies": {
    "@google-cloud/spanner": "8.3.1",
    "@sebspark/cli-tester": "*",
    "@sebspark/tsconfig": "*",
    "@types/yargs": "17.0.35",
    "testcontainers": "11.9.0"
  },
  "peerDependencies": {
    "@google-cloud/spanner": "*"
  },
  "dependencies": {
    "@inquirer/prompts": "8.0.2",
    "typescript": "5.9.3",
    "yargs": "18.0.0"
  }
}
