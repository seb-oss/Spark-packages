{
  "name": "@sebspark/promise-cache",
  "version": "6.0.0",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "type": "module",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup-node src/index.ts --format esm --target node22 --dts",
    "dev": "tsc --watch --noEmit",
    "lint": "biome check .",
    "test": "vitest run --passWithNoTests --coverage",
    "test:e2e": "vitest --config vitest.config.e2e.ts --run",
    "typecheck": "vitest --typecheck.only --passWithNoTests"
  },
  "devDependencies": {
    "@sebspark/otel": "*",
    "@sebspark/tsconfig": "*",
    "@testcontainers/redis": "11.7.2",
    "testcontainers": "11.7.2"
  },
  "dependencies": {
    "date-fns": "4.1.0",
    "fix-esm": "1.0.1",
    "redis": "5.9.0",
    "superjson": "2.2.5"
  },
  "peerDependencies": {
    "@sebspark/otel": ">=2.0.0"
  }
}
