{
  "name": "sebspark",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "scripts": {
    "build": "turbo build",
    "changeset": "changeset",
    "dev": "turbo dev",
    "format": "biome format --write",
    "generate:package": "turbo gen package",
    "lint": "turbo lint",
    "lint:watch": "turbo watch lint",
    "release": "npm run build && npx changeset publish",
    "test": "turbo test",
    "test:watch": "turbo watch test",
    "typecheck": "turbo typecheck",
    "clean": "rm -rf ./packages/**/dist",
    "start": "turbo watch lint test typecheck"
  },
  "private": true,
  "dependencies": {
    "@changesets/cli": "2.27.5"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.0",
    "@turbo/gen": "2.0.1",
    "@types/node": "20.14.2",
    "@vitest/coverage-v8": "1.6.0",
    "csv-parse": "5.5.6",
    "fast-xml-parser": "4.4.0",
    "tsup": "8.1.0",
    "turbo": "2.0.12",
    "typescript": "5.4.5",
    "vitest": "1.6.0"
  },
  "resolutions": {
    "semver": ">=7.5.4",
    "word-wrap": ">=1.2.5"
  },
  "workspaces": [
    "packages/*"
  ],
  "packageManager": "yarn@1.22.19"
}
