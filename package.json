{
  "name": "sebspark",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "scripts": {
    "build": "turbo build",
    "changeset": "changeset",
    "dev": "turbo dev",
    "format": "biome format --write .",
    "generate:package": "turbo gen package",
    "lint": "biome check --apply .",
    "release": "npm run build && npx changeset publish",
    "test": "FORCE_COLOR=1 turbo test",
    "typecheck": "FORCE_COLOR=1 turbo typecheck",
    "clean": "rm -rf ./packages/**/dist"
  },
  "private": true,
  "dependencies": {
    "@changesets/cli": "2.27.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.7.3",
    "@turbo/gen": "1.13.3",
    "@types/node": "20.12.8",
    "@vitest/coverage-v8": "1.5.3",
    "csv-parse": "5.5.5",
    "fast-xml-parser": "4.3.6",
    "tsup": "8.0.2",
    "turbo": "1.13.3",
    "typescript": "5.4.5",
    "vitest": "1.5.3"
  },
  "resolutions": {
    "semver": ">=7.5.4",
    "word-wrap": ">=1.2.5"
  },
  "workspaces": [
    "packages/*"
  ],
  "packageManager": "yarn@1.22.19"
}
